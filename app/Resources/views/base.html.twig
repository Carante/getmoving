<!DOCTYPE html>
<html>
<head>
	<meta charset="UTF-8"/>
	<title>{% block title %}Welcome!{% endblock %}</title>
	<link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}"/>

	{% block stylesheets %}
		<link rel="stylesheet" href="{{ asset('css/normalizer.min.css') }}">
		<link rel="stylesheet" href="{{ asset('css/bootstrap.min.css') }}">
		<link rel="stylesheet" href="{{ asset('css/bootstrap-theme.min.css') }}">
		<link rel="stylesheet" href="{{ asset('css/font-awesome.min.css') }}">
		<link rel="stylesheet" href="{{ asset('css/jquery-ui.min.css') }}">
		<link rel="stylesheet" href="{{ asset('css/sweetalert2.min.css') }}">
		<link rel="stylesheet" href="{{ asset("css/style.css") }}">
	{% endblock %}
</head>
<body>
{% block menu %}
	<nav class="GM-navbar">
		<div class="container-fluid">
			<!-- Brand and toggle get grouped for better mobile display -->
			<div class="navbar-header">
				<button type="button" class="navbar-toggle collapsed" data-toggle="collapse"
				        data-target="#GM-collapsed-menu" aria-expanded="false">
					<span class="sr-only">Toggle navigation</span>
					<span class="icon-bar"></span>
					<span class="icon-bar"></span>
					<span class="icon-bar"></span>
				</button>
				<a class="navbar-brand" href="{{ path('home') }}"><img src="{{ asset('media/images/logo.png') }}" alt="GetMoving Fiji logo" width="125px"></a>
			</div>

			<!-- Collect the nav links, forms, and other content for toggling -->
			<div class="collapse navbar-collapse navbar-right" id="GM-collapsed-menu">
				<ul class="GM-nav">
					<li><a href="{{ path('contact') }}">Get in touch</a></li>
					<li><a href="{{ path('why') }}">Why Us?</a></li>
					<li><a href="{{ path('programs_list') }}">Programs</a></li>
					<li><a href="{{ path('user_login') }}">Login</a></li>
				</ul>
			</div><!-- /.navbar-collapse -->
		</div><!-- /.container-fluid -->
	</nav>
{% endblock %}

{% block body %}{% endblock %}

{% block javascripts %}
	<script src="{{ asset('js/jquery-3.1.1.slim.min.js') }}"></script>
	<script src="{{ asset('js/jquery-ui.min.js') }}"></script>
	<script src="{{ asset('js/bootstrap.min.js') }}"></script>
	<script src="{{ asset('js/sweetalert2.min.js') }}"></script>
	<script src="{{ asset('js/script.js') }}"></script>
{% endblock %}
</body>
</html>
