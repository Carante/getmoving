<div class="GM-full-wrap">
  <div class="GM-fullbackground GM-video">
    <video width="100%" height="100%" poster="{{ asset('media/images/rakiraki.JPG') }}" class="fullscreen-bg__video"
           autoplay loop muted>
      <source src="{{ asset('media/videos/flyby.mp4') }}" type="video/mp4">
      Your browser does not support the video tag.
    </video>
  </div>
  <div class="GM-register-wrap text-center text--shadow">
    <a href="{{ path('programs_list') }}">
      <button class="ghost-btn ghost-btn-light ghost-btn-lg">SEE THE PROGRAMS</button>
