  {{ form_start(form) }}
  <h2>Contact details</h2>
  <div class="row">
    <div class="col-md-4">
      {{ form_row(form.email) }}
    </div>
    <div class="col-md-4">
      {{ form_row(form.phone) }}
    </div>
  </div>

  <h2>Contact address</h2>
  <div class="row">
    <div class="col-md-4">
      {{ form_row(form.addressCountry) }}
    </div>
    <div class="col-md-4">
      {{ form_row(form.addressRegion) }}
    </div>
  </div>
  <div class="row">
    <div class="col-md-4">
      {{ form_row(form.addressZip) }}
    </div>
    <div class="col-md-4">
      {{ form_row(form.addressCity) }}
    </div>
  </div>
  <div class="row">
    <div class="col-md-4">
      {{ form_row(form.addressStreet) }}
    </div>
    <div class="col-md-4">
      {{ form_row(form.addressPoBox) }}
    </div>
  </div>
  <div class="row">
    <div class="col-md-4">
      {{ form_row(form.addressHouseNo) }}
    </div>
    <div class="col-md-4">
      {{ form_row(form.addressCo) }}
    </div>
  </div>

  <h2>Education levels</h2>
  <div class="row">
    <div class="col-md-4">
      {{ form_row(form.eduLevelExpected) }}
    </div>
  </div>
  <div class="row">
    <div class="col-md-4">
      {{ form_row(form.eduCurrentPlace) }}
    </div>
    <div class="col-md-4">
      {{ form_row(form.eduCurrentProgram) }}
    </div>
  </div>
  <div class="row">
    <div class="col-md-4">
      {{ form_row(form.eduFuturePlace) }}
    </div>
    <div class="col-md-4">
      {{ form_row(form.eduFutureProgram) }}
    </div>
  </div>
  <div class="row margin--top-l">
    <div class="col-md-8">
      <button class="btn btn-success" type="submit">Save <i class="fa fa"></i></button>
      {{ form_end(form) }}
    </div>
    <div class="col-xs-12">
    </div>
  </div>
